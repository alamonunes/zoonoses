 <h1>Welcome <%= current_cuidador.nome %>!</h1>
 <%= link_to "Log out", destroy_cuidador_session_path, :method => :delete %>
 
<p id="notice"><%= notice %></p>

<h1>Cuidadors</h1>

<table>
  <thead>
    <tr>
      <th>Cpf</th>
      <th>Nome</th>
      <th>Rg</th>
      <th>Sexo</th>
      <th>Telefone</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cuidadors.each do |cuidador| %>
      <tr>
        <td><%= cuidador.cpf %></td>
        <td><%= cuidador.nome %></td>
        <td><%= cuidador.rg %></td>
        <td><%= cuidador.sexo %></td>
        <td><%= cuidador.telefone %></td>
        <td><%= cuidador.data %></td>
        <td><%= link_to 'Show', cuidador %></td>
        <td><%= link_to 'Edit', edit_cuidador_path(cuidador) %></td>
        <td><%= link_to 'Destroy', cuidador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
