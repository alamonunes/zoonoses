 <h1>Welcome <%= current_funcionario.nome %>!</h1>
 <%= link_to "Log out", destroy_funcionario_session_path, :method => :delete %>

<p id="notice"><%= notice %></p>

<h1>Funcionarios</h1>

<table>
  <thead>
    <tr>
      <th>Cpf</th>
      <th>Nome</th>
      <th>Rg</th>
      <th>Sexo</th>
      <th>Telefone</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @funcionarios.each do |funcionario| %>
      <tr>
        <td><%= funcionario.cpf %></td>
        <td><%= funcionario.nome %></td>
        <td><%= funcionario.rg %></td>
        <td><%= funcionario.sexo %></td>
        <td><%= funcionario.telefone %></td>
        <td><%= funcionario.data %></td>
        <td><%= link_to 'Show', funcionario %></td>
        <td><%= link_to 'Edit', edit_funcionario_path(funcionario) %></td>
        <td><%= link_to 'Destroy', funcionario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
